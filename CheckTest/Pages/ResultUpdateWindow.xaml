<Window x:Class="CheckTest.Pages.ResultUpdateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CheckTest.Pages"
        mc:Ignorable="d"
        Title="ResultUpdateWindow" Height="200" Width="200" Background="Linen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Width="Auto" x:Name="ResultBox" PreviewTextInput="ResultBox_PreviewTextInput"></TextBox>
        <Button Grid.Row="1" Click="Button_Click">Редактировать</Button>
    </Grid>
</Window>
